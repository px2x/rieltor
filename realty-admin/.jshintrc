{
    "curly": true,
    "camelcase": true,
    "eqeqeq": true,
    "freeze": true,
    "indent": 4,
    "newcap": true,
    "quotmark": "single",
    "maxdepth": 5,
    "maxstatements": 40,
    "maxlen": 120,
    "eqnull": true,
    "funcscope": true,
    "unused": "vars",
    "undef": true,
    "browser": true,
    "predef": [
        "$",
        "angular",
        "console",
        "alert",
        "confirm",
        "window"
    ],
    "latedef": "nofunc",
    "globals" : {
        "module": false,
        "require": false,
        "__dirname": false,
        "process": false
    }
}
